  "main": "index.js",
  "scripts": {
    "develop": "yarn run develop:11ty & yarn run develop:webpack",
    "develop:11ty": "eleventy --watch --serve --port=8081",
    "develop:webpack": "webpack --watch --config webpack.development.config.js --output-path=./_compiled",
    "build": "yarn run build:webpack && yarn run build:11ty",
    "build:11ty": "eleventy --config=.eleventy.production.js",
    "build:webpack": "webpack --config webpack.production.config.js --output-path=./_compiled",
